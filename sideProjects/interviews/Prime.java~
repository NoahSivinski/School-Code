public class Prime {
    public static void main (String[] args) {
	int count = 0;
	int num = 0;
	while (count < 10001) {
	    if (isPrime(num)) {
		count++;
	    } // if
	    num++;
	} // while
	System.out.println(num)
    } // main

    public boolean isPrime(int num) {
	 boolean flag = false;
    for (int i = 2; i <= num / 2; ++i) {
      // condition for nonprime number
      if (num % i == 0) {
        flag = true;
        break;
      }
      
      return flag
    } // isPrime
} // Prime
